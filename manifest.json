{
  "name": "Gamer Baby",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Replaces Baby with Gamer Baby for your lovely web experience.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
  ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
